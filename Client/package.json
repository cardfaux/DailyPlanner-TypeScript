{
  "name": "dailyplanner",
  "version": "1.0.0",
  "private": true,
  "description": "DailyPlanner",
  "scripts": {
    "clear-build-cache": "rm -rf .cache/ development/",
    "dev": "parcel public/index.html --out-dir development -p 3000",
    "devOpen": "parcel public/index.html --out-dir development -p 3000 --open",
    "tsc": "tsc --watch --noEmit",
    "build": "parcel build public/index.html --out-dir production --public-url ./",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "tsPrettier": "prettier --write \"src/**/*.{ts,tsx,json,html,css,scss}\"",
    "tsLint": "tslint --fix --project tsconfig.json \"src/**/*.{ts,tsx}\""
  },
  "keywords": [
    "React",
    "Parcel"
  ],
  "author": "James Hagood",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/runtime-corejs2": "^7.8.7",
    "@types/node": "^13.9.1",
    "@types/react": "^16.9.23",
    "@types/react-big-calendar": "^0.24.0",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/react-toast-notifications": "^2.4.0",
    "@types/react-transition-group": "^4.2.4",
    "@types/styled-components": "^5.0.1",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-plugin-styled-components": "^1.10.7",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-config-react-app": "^5.2.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1",
    "sass": "^1.26.3",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@reach/router": "^1.3.3",
    "moment": "^2.24.0",
    "react": "^16.13.0",
    "react-big-calendar": "^0.24.1",
    "react-dnd": "^10.0.2",
    "react-dnd-html5-backend": "^10.0.2",
    "react-dom": "^16.13.0",
    "react-icons": "^3.9.0",
    "react-is": "^16.13.0",
    "react-moment": "^0.9.7",
    "react-router-dom": "^5.1.2",
    "react-spring": "^8.0.27",
    "react-toast-notifications": "^2.4.0",
    "react-transition-group": "^4.3.0",
    "styled-components": "^5.0.1",
    "uuid": "^7.0.2"
  },
  "extends": "react-app",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 ChromeAndroid versions",
    "last 2 Firefox versions",
    "last 2 FirefoxAndroid versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 Edge versions",
    "last 2 Opera versions",
    "last 2 OperaMobile versions"
  ]
}
